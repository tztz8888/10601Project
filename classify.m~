function [Y]= classify(Model,X)
%k nearest neighbor
K=20;
nLabel=10;
feature= ExtractFeature(X);
nTest= length(feature(1,:) );
nFeature= length(feature(:,1) );
nTrain= length(Model(:,1));

distMetrics= zeors(nTest,nTrain);
for i=1:nTest
    for j=1:nTrain
         distMetrics(i,j)= L2Distance( feature(i,:),Model(i,1:nFeature) );
    end
end

for i=1:nTest
    flagKNN= zeros(K,1);
    distKNN=zeors(K,1);
    for j=1:K
       distKNN(j)=2^8-1;
    end
    for j=1:nData
        for k=1:K
            if distMetrics(i,j)<distKNN(k)
                for m=K:-1:k+1
                    distKNN(m)=distKNN(m-1);
                end
                distKNN(k)= distMetrics(i,j);
                flagKNN(k)= Model(j,nFeature+1);
                break;
            end
        end
    end
   
    maxCount=0;
    idxMaxCount=0;
    count= zeros(nLabel,1);
    for j=1:K
        count
    
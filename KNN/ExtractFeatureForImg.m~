function [ featureVecs ] = ExtractFeatureForImg( data ,cluster)
% Extract a 50-dimension feature for each img
% input: a 1*3072 img
% output: a 1*50 feature vec
    load(X);
    featureVecs=zeros();
    for i=1:size(data,1)
        descrp=GenerateDenseSiftForImg(img);
        [drop, binsa] = min(vl_alldist(vocab, single(descrp)), [], 1) ; 
        hist=zeros(1,vocab_size);
        for k=1:vocab_size
            hist(k)=sum(binsa==k);
        end
    end



end

